    <main>
      <body>
        <div class="show-cats">
          <h2>所属猫詳細(管理者)</h2>
          <div class="product-img" style="margin-top: 20px;">
            <%= image_tag @cat.image, size: "300x200" %>
          </div>
        </div>
          <table class="table table-hover table-inverse">
            <tbody>
                <td>名前</td>
                <td><%= @cat.name %></td>
              </tr>
              <tr>
                <td>特徴</td>
                <td><%= @cat.introduction %></td>
              </tr>
              <tr>
                <td>性別</td>
                <td><%= @cat.genders %></td>
              </tr>
              <tr>
                <td>種類</td>
                <td><%= @cat.cat_type %></td>
              </tr>
              <tr>
                <td>年齢</td>
                <td><%= @cat.age %></td>
              </tr>
              <tr>
                <td>出勤時間</td>
              </tr>
            </tbody>
          </table>
          <div class="col-md-offset-1 col-md-2 text-right">
            <%= link_to "編集する" ,edit_admin_cat_path(@cat), class: "btn btn-outline-dark" %>
          </div>
        </div>
        <div class="row">
          <div class="col-xs-2">
          </div>
          <div class="col-xs-5">
          </div>
        </div>
      </body>
    </main>
